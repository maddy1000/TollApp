@page
@model TollApp.Pages.OPERATOR.DisplayAllTransactionsModel
@{
}


 <table class="table">
     
   


    
            <thead>
                <tr>
                    <th scope="col">Toll Id</th>
                    <th scope="col">Toll Location</th>
                      <th scope="col">User Id</th>
                       <th scope="col">User Name</th>
                    <th scope="col">Amount</th>
                       <th scope="col">Transaction Date</th>
                    
                   

                </tr>
            </thead>
            <tbody>
                @if(Model.UserRecords == null || Model.UserRecords.Count <=0)
                {
                    <tr>
                        <td>"No data found"</td>
                    </tr>
                }
                else
                {
                @foreach (var d in Model.UserRecords)
                {
                    <tr>
                        <td>@d.TollId</td>
                        <td>@d.TollLocation</td>
                        <td>@d.UserId</td>
                        <td>@d.UserName</td>
                        <td>@d.Amount</td>
                        <td>@d.TransactionDate</td>
                      
                    

                
                       
                    </tr>
                }
                }
            </tbody>
        </table>